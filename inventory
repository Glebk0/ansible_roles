[webservers]
webserver ansible_host=192.168.1.70 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/webserver/virtualbox/private_key

[webservers:vars]
ansible_user=devops

[appservers]
appserver ansible_host=192.168.1.71 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/appserver/virtualbox/private_key

[appservers:vars]
ansible_user=devops

[servers:children]
webservers
appservers

[servers:vars]
# ansible_ssh_private_key_file=/home/student/ansible/day2/devops.pem